{{>navbar}}


<div class="col-md-7 mx-auto col-12 px-0 pt-3">
  {{>header title = "SunDial"}}

  <nav class="text-center navbar navbar-default mx-0 px-0">
    <button type="button" class="py-2 col-12 navbar-toggle collapsed selUser bg-info text-white" data-toggle="collapse"
      data-target="#codebrainery-toggle-nav" aria-expanded="false">
      Select User
    </button>
    <div class="collapse navbar-collapse" id="codebrainery-toggle-nav">
      <ul class="mx-2 list-group userListEl list-group-flush">
        {{#each users}}
        <li data-id={{id}} class="list-group-item userInfo">{{firstName}} {{lastName}}</li>
        {{/each}}
      </ul>
    </div>
  </nav> <!-- close navbar nav -->
  
</div>